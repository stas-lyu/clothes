{
  "scripts": {
    "start-json-server": "json-server --watch db.json --port 3030",
    "start-webpack-dev-server": "webpack-dev-server --config webpack.config.js --open",
    "build": "webpack",
    "start": "webpack --watch",
    "dev": "concurrently --kill-others \"npm run start\" \"npm run start-json-server\""
  },
  "dependencies": {
    "json-server": "^0.16.3",
    "webpack": "^5.11.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "css-loader": "^5.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "mini-css-extract-plugin": "^1.3.3",
    "node-sass": "^5.0.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "webpack-cli": "^4.2.0"
  }
}
